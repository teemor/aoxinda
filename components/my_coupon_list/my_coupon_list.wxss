@import "../../app.wxss";
.my_coupon_list_contain{
    border-radius: 20rpx;
    margin: 35rpx 30rpx 30rpx 0rpx;
    height: 115rpx;
    padding: 5rpx 20rpx 5rpx 30rpx;
    box-shadow: 0px 0px 25rpx #a7a6a6c4;
    width: 68%;
}
.my_coupon_list_text{
    margin-top:10rpx;
}
.my_coupon_list_color{
    color: #fff;
    position: relative;
    width:20%;
}
.my_coupon_list_color image{
    margin: 30rpx 0 30rpx 0rpx;
    padding: 5rpx 10rpx 5rpx 30rpx;
    height:125rpx;
    width:100%;
}
.my_coupon-button{
    margin-top:-18rpx;
    width:150rpx;
    height:50rpx;
    line-height: 50rpx;
    border-radius: 30rpx;
    border:2rpx solid #fb6262;
}