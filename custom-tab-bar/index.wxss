.com-tabbar {
  -webkit-transform: translateY(0);
  transform: translateY(0);
  background: #fff;
  z-index: 111;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  height: 100rpx;
  display: flex;
  border-bottom: 1rpx solid #eee;
}

.com-tabbar-item {
  flex: 1;
  text-align: center;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  color: #333;
}

.com-tabbar-icon {
  width: 56rpx;
  height: 56rpx;
}

.com-tabbar-icon-super {
  position: fixed;
  z-index: 222;
  margin-bottom: 50rpx;
  width: 140rpx;
  height: 140rpx;
}

.com-tabbar-text {
  margin-top: 8rpx;
  font-size: 20rpx;
  line-height: 20rpx;
}

.com-tabbar-item_active {
  color: #4e93f3;
}

.contain-tab {
  z-index: 2222;
  border: 1px solid #e4e4e4;
  margin-left: 10px;
  background: #fff;
  bottom: 130rpx;
  position: fixed;
}

.contain-nav {
  border-bottom: 1px solid #e4e4e4;
  font-size: 30rpx;
  background: #fff;
  width: 250rpx;
  height: 80rpx;
  text-align: center;
  line-height: 80rpx;
  z-index: 999;
}

.contain-nav:last-child {
  border: none;
}
