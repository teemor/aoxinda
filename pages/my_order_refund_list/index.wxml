<van-tabs active="{{active}}" bind:change="tabchange" color="#4e93f3">
  <van-tab title="商城">
    <view wx:if="{{refundList.backOrderData.length===0}}">
      <view class="my_address_null">
        <image src="../../common/image/refund_null.png" />
      </view>
      <view class="tc fz26">您对购买的商品都很满意</view>
    </view>
    <view wx:else>
      <view wx:for="{{refundList.backOrderData}}" wx:key="{{item}}" bindtap="refundDetail" data-item="{{item.id}}">
        <view class="br_btm_light" style="padding:30rpx">
          <view class="fl my_order_list_image">
            <image src="{{item.file_url}}" />
          </view>
          <view class="fl my_order_list_text fz28">
            <view class="fz28 order_text_over">{{item.goods_name}}</view>
            <view class="fz24 my_order_describe">数量{{item.goods_num}} 产品：{{item.sku_name}}</view>
            <view class="cl">
              <view class="fl red">¥{{item.back_money}}</view>
              <view class="fr">退款数量X{{item.goods_num}}</view>
            </view>
          </view>
        </view>
        <van-cell title="{{item.order_type+' '+item.back_type}}" value="" is-link></van-cell>
      </view>
    </view>
  </van-tab>


  <van-tab title="洗车美容">
    <view wx:if="{{refundClean.length===0}}">
      <view class="my_address_null">
        <image src="../../common/image/refund_null.png" />
      </view>
      <view class="tc fz26">您对购买的商品都很满意</view>
    </view>
    <view wx:else>
      <view wx:for="{{refundClean}}" wx:key="{{item}}" bindtap="refundDetail" data-item="{{item.id}}">
        <view class="br_btm_light" style="padding:30rpx">
          <view class="fl my_order_list_image">
            <image src="{{item.pictue}}" />
          </view>
          <view class="fl my_order_list_text fz28">
            <view class="fz28 order_text_over">{{item.actName}}</view>
            <view class="cl">
              <view class="fl red">¥{{item.refundPrice}}</view>
              <view class="fr">退款数量X{{item.goodsNum}}</view>
            </view>
          </view>
        </view>
        <van-cell title="{{item.refundState==14?'退款成功':'退款失败'}}" value="" is-link></van-cell>
      </view>
    </view>
  </van-tab>
  <van-tab title="汽车救援">
    <view wx:if="{{refundList.backOrderData.length===0}}">
      <view class="my_address_null">
        <image src="../../common/image/refund_null.png" />
      </view>
      <view class="tc fz26">您对购买的商品都很满意</view>
    </view>
    <view wx:else>
      <view wx:for="{{refundList.backOrderData}}" wx:key="{{item}}" bindtap="refundDetail" data-item="{{item.id}}">
        <view class="br_btm_light" style="padding:30rpx">
          <view class="fl my_order_list_image">
            <image src="{{item.file_url}}" />
          </view>
          <view class="fl my_order_list_text fz28">
            <view class="fz28 order_text_over">{{item.goods_name}}</view>
            <view class="fz24 my_order_describe">数量{{item.goods_num}} 产品：{{item.sku_name}}</view>
            <view class="cl">
              <view class="fl red">¥{{item.back_money}}</view>
              <view class="fr">退款数量X{{item.goods_num}}</view>
            </view>
          </view>
        </view>
        <van-cell title="{{item.order_type+' '+item.back_type}}" value="" is-link></van-cell>
      </view>
    </view>
  </van-tab>

</van-tabs>