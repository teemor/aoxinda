<view>
  <view class="banner">
    <image src="../../common/img/banner.png" />
  </view>
  <view class="cl fz32 keepup">
    <view class="fl  tc bold" style="width:180rpx;border-bottom:4rpx solid #fdbb51;">
      附近门店
    </view>
    <view class="fl fz30 grey">
      <view class="fl loc_img">
        <image src="../../common/img/location.png" />
      </view>
      <view class="fl">人民购物广场</view>
    </view>
    <view class="fr grey fz26" bindtap="storeList">
      <view class="fl">全部</view>
      <view class="fl all_img">
        <image src="../../common/img/all.png" />
      </view>
    </view>
  </view>
  <view wx:if="carType" class="shop_goods_invite" bindtap="editCar">
    <image src="http://maichefu.oss-cn-beijing.aliyuncs.com/ToShop/shop_bg.png" mode="aspectFit" />
    <view class="shop_goods_invite_text">为您推荐适合 {{carType.model}}{{carType.vehicleType}}...的商品</view>
    <view class="shop_edit">
      <image src="../../../common/image/edit.png" />
    </view>
    <view class="shop_close">
      <image src="../../../common/image/close.png" />
    </view>
  </view>
  <view class="cl space_contain">
    <view class="space_box tc fl">
      <view class="space_img">
        <image src="../../common/img/space.png" />
      </view>
      <view class="fz30">全部</view>
    </view>
    <view class="space_box tc fl">
      <view class="space_img">
        <image src="../../common/img/space.png" />
      </view>
      <view class="fz30">标准洗车</view>
    </view>
    <view class="space_box tc fl">
      <view class="space_img">
        <image src="../../common/img/space.png" />
      </view>
      <view class="fz30">精致洗车</view>
    </view>
    <view class="space_box tc fl">
      <view class="space_img">
        <image src="../../common/img/space.png" />
      </view>
      <view class="fz30">打蜡</view>
    </view>
    <view class="space_box tc fl">
      <view class="space_img">
        <image src="../../common/img/space.png" />
      </view>
      <view class="fz30">打蜡</view>
    </view>
  </view>
  <view class="space_list fz28 cl">
    <view class="space_store_box cl" wx:for="{{CleanStore}}" wx:key="{{item}}">
    <storeList item="{{item}}"/>
    </view>
  </view>
</view>